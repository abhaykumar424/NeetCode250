class Solution {
public:
    bool isAnagram(string s, string t) {
        unordered_map<char, int>mp1;
        unordered_map<char, int>mp2;
        int n = s.size();
        int m = t.size();
        if(n!=m) return false;
        for(int i=0; i<n; i++){
            mp1[s[i]]++;
            mp2[t[i]]++;
        }
        for(auto it : mp1){
            if(mp2[it.first]!=it.second) return false;
        }
        return true;
    }
};