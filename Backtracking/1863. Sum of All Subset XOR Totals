class Solution {
public:
    int dfs(vector<int>& nums , int i, int total){
        if(i == nums.size()){
            return total;
        }
        return dfs(nums, i+1, nums[i]^total) + dfs(nums, i+1, total);
    }
    int subsetXORSum(vector<int>& nums) {
        return dfs(nums, 0, 0);
    }
};