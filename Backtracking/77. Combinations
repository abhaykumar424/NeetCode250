class Solution {
public:
    void dfs(int n, vector<vector<int>>&ans , vector<int>& subset, int k, int start){
        
        if(subset.size()==k){
            ans.push_back(subset);
            return;
        }
        if(start>n) return;
        subset.push_back(start);
        dfs(n,ans, subset, k, start+1);
        subset.pop_back();
        dfs(n, ans, subset, k, start+1);
    }
    vector<vector<int>> combine(int n, int k) {
        vector<int>subset;
        vector<vector<int>>ans;
        dfs(n, ans, subset, k, 1);
        return ans;
    }
};